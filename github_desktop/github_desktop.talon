app: github_desktop
-
open [repo]:
  key(ctrl-shift-a)
  
git pull:
  key(ctrl-shift-p)

git push:
  key(ctrl-p)

[show|list] branches:
  key(ctrl-b)

new repo:
  key(ctrl-n)

clone [repo]:
  key(ctrl-shift-o)

[show|list] repos:
  key(ctrl-t)

new branch:
  key(ctrl-shift-n)

exit|quite|close:
  key(alt-f4)